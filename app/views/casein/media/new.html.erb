<h2>Add a new media</h2>

<%= form_for @media, :url => casein_media_index_path do |f| %>
	
	<%= render :partial => 'casein/media/form', :locals => { :f => f } %>
	
	<p class="submits">
		<%= link_to 'Cancel', casein_media_index_path, :class => 'btn btn-mini btn-danger' %>
